---
import { Separator } from "@/features/classes/components/molecules";
import {
  BuyingGuide,
  CallToAction,
  HeroProducts,
  ListProducts,
  TrustedBrands,
} from "@/features/products/components/organisms";
import Layout from "../core/components/templates/layout.astro";

const title = "Productos | Morros Training";
const description =
  "Descubre nuestras clases de entrenamiento grupales, calendario, beneficios y más en Morros Training. Únete a nuestras clases hoy mismo.";
const keywords =
  "productos, entrenamiento, fitness, grupo, calendario, beneficios";
---

<Layout title={title} description={description} keywords={keywords}>
  <HeroProducts />
  <Separator />
  <ListProducts />
  <TrustedBrands />
  <BuyingGuide />
  <CallToAction />
</Layout>
